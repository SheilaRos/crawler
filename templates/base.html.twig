<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        <link rel="icon" type="" sizes="16x16" href="https://d1pwix07io15pr.cloudfront.net/v5a259d51b6/favicons/favicon-16x16.png?">
        {% block stylesheets %}
            <link href="{{ asset('css/bootstrap.min.css') }}" type="text/css" rel="stylesheet" />
            <link href="{{ asset('css/main.css') }}" type="text/css" rel="stylesheet" />
            <link href="{{ asset('css/main.css') }}" type="text/css" rel="stylesheet" />
            <link href="{{ asset('css/all.css') }}" type="text/css" rel="stylesheet" />
            <link href="{{ asset('css/fontawesome.css') }}" type="text/css" rel="stylesheet" />


        {% endblock %}
    </head>
    <body>
    <div class="self-building-square-spinner spinner-conf">
        <div class="square"></div>
        <div class="square"></div>
        <div class="square"></div>
        <div class="square clear"></div>
        <div class="square"></div>
        <div class="square"></div>
        <div class="square clear"></div>
        <div class="square"></div>
        <div class="square"></div>
    </div>
        {% block body %}{% endblock %}
        {% block javascripts %}
            <script src="{{ asset('js/jquery-3.3.1.min.js') }}" type="text/javascript"></script>
            <script src="{{ asset('js/popper.js') }}" type="text/javascript"></script>
            <script src="{{ asset('js/bootstrap.min.js') }}" type="text/javascript"></script>
            <script src="{{ asset('js/main.js') }}" type="text/javascript"></script>
            <script src="{{ asset('js/all.js') }}" type="text/javascript"></script>
            <script src="{{ asset('js/fontawesome.js') }}" type="text/javascript"></script>

        {% endblock %}
